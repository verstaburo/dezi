extends ../../layouts/case

block head
  - var pageTitle = 'Dezi | 4mebeles'

block header
  - var headerClass = '_light'
  - var headerData = getData('headerAgrosyn.json')

block content
  +mebeles(data-section="portfolio")
    +e.wall(data-scrolly-top)
      +mebeles-about
      +mebeles-logo
